Inner join-----------------------------------------------------------------------
SELECT table1.col1,table2.col2
FROM table1
INNER JOIN table2
ON table1.col1 = table2.col2;

Left join-------------------------------------------------------------------------
SELECT table1.col1,table2.col2
FROM table1
LEFT JOIN table2
ON table1.col1 = table2.col2;

Right outer join------------------------------------------------------------------
SELECT table1.col1,table2.col2
FROM table1
right outer JOIN table2
ON table1.col1 = table2.col2;

Full outer join-------------------------------------------------------------------
SELECT table1.col1,table2.col2
FROM table1
full outer JOIN table2
ON table1.col1 = table2.col2;
